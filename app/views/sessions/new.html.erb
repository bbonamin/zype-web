<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_tag('/sessions') do %>
  <%= label_tag 'username' %>
  <%= text_field_tag 'username' %>

  <br>

  <%= label_tag 'password' %>
  <%= password_field_tag 'password' %>

  <br>
  <%= hidden_field_tag 'origin_video_id', @origin_video_id %>
  <%= submit_tag 'Log in' %>
<% end %>
